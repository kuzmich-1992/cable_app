

<h4>current users</h4>
<ul id="member-list">
  <% @room_users.each do |member| %>
    <li><%= member.name %></li>
  <% end %>
</ul>

<h4>Add Members</h4>
<ul id="add-users">
<% @users.each do |user| %>
  <li>
    <%= user.username %>
    <input type="checkbox" id="<%= "#{user.id}-add-member" %>"/>
  </li>
<% end %>
</ul>